<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cherry.privilege.dao.MenusMapper">
	<select id="getById" resultType="com.cherry.privilege.api.pojo.Menus"
		parameterType="java.lang.Integer">
		select * from menus where id = #{id}
	</select>

	<insert id="saveMenu" keyProperty="id" useGeneratedKeys="true"
		parameterType="com.cherry.privilege.api.pojo.Menus">
		
	</insert>
</mapper>